automation "click" {
    navigate {
        url = "https://example.com"
    }

    click {
        selector = "#olive"
    }

    click {
        selector {
            target = "#raspberry"
        }
    }

    click {
        selector {
            target = "#raspberry"
            by     = "SEARCH"
            on     = "VISIBLE"
        }
    }

    double_click {
        selector {
            target = "#yellow"
            by     = "SEARCH"
            on     = "VISIBLE"
        }
    }
}